{
    "manifestVersion": 1,
    "id": "flutter",
    "version": "0.2.41",
    "name": "Flutter Tasks",
    "description": "Flutter extension for Azure DevOps. Active-Updated Fork of aloisdeniel/vsts-flutter-tasks",
    "publisher": "hey24sheep",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"
    },
    "screenshots": [],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/aloisdeniel/vsts-flutter-tasks/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/hey24sheep/vsts-flutter-tasks"
    },
    "tags": [
        "flutter",
        "mobile",
        "ios",
        "android",
        "dart"
    ],
    "files": [
        {
            "path": "tasks/install"
        },
        {
            "path": "tasks/build"
        },
        {
            "path": "tasks/test"
        },
        {
            "path": "tasks/analyze"
        },
        {
            "path": "tasks/command"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "flutter-install",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/install"
            }
        },
        {
            "id": "flutter-build",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/build"
            }
        },
        {
            "id": "flutter-test",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/test"
            }
        },
        {
            "id": "flutter-analyze",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/analyze"
            }
        },
        {
            "id": "flutter-command",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/command"
            }
        }
    ]
}